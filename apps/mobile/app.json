{
  "expo": {
    "name": "Plan2Skill",
    "slug": "plan2skill",
    "version": "0.1.0",
    "scheme": "plan2skill",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0C0C10"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.plan2skill.app",
      "usesAppleSignIn": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0C0C10"
      },
      "package": "com.plan2skill.app"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-apple-authentication"
    ]
  }
}
